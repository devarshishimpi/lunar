{
  "name": "lunar-theme",
  "displayName": "Lunar VSCode Theme",
  "description": "A minimalist VS Code theme that makes your code shine like the moon",
  "version": "1.2.0",
  "icon": "lunarlogo128.png",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/prismlinkapp/lunar-vscode-theme.git"
  },
  "publisher": "PrismLink",
  "contributes": {
    "themes": [
      {
        "label": "Lunar Dark Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/lunar-dark-color-theme.json"
      },
      {
        "label": "Lunar Light Theme",
        "uiTheme": "vs",
        "path": "./themes/lunar-light-color-theme.json"
      }
    ]
  },
  "dependencies": {
    "@vscode/vsce": "^2.24.0"
  }
}
